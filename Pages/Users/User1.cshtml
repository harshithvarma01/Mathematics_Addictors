@page
@model User1Model
@{
    ViewData["Title"] = "Probability";
}

<h1>Probability</h1>
<h2>                                       </h2>

@for (int i = 0; i < Model.Questions.Count; i++)
{
    // Replace "1" with the appropriate box ID for your questions.
    <a class="question-box" asp-page="/QuestionDetailsProb/QuestionDetail" asp-route-boxId="1" asp-route-questionId="@i">
        <span class="question-number">@((i + 1).ToString())</span>
        <p class="question-text">@Model.Questions[i]</p>
    </a>
}